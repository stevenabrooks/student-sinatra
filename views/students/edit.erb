<h1>Edit a Student</h1>
  

<h3>Change below if you want</h3>
<form action="/students/<%= @student.id %>" method="POST">

  <label>Name</label>
    <input type="TEXT" name="name" value="<%= @student.name %>"/><br>
    
  <label>Url</label>
    <input type="TEXT" name="url" value="<%= @student.url %>"/><br>

  <label>Img</label>  
    <input type="TEXT" name="img" value="<%= @student.img %>"/><br>

  <label>Tagline</label>  
    <input type="TEXT" name="tagline" value="<%= @student.tagline %>"/><br>  

  <label>Bio</label>
    <input type="TEXT" name="bio" value="<%= @student.bio %>"/><br>
    
  <label>Prof Pic</label>
    <input type="TEXT" name="prof_pic" value="<%= @student.prof_pic %>"/><br>

  <label>Twitter</label>  
    <input type="TEXT" name="twitter" value="<%= @student.twitter %>"/><br>

  <label>Linkedin</label>  
    <input type="TEXT" name="linkedin" value="<%= @student.linkedin %>"/><br>    

  <label>Github</label>
    <input type="TEXT" name="github" value="<%= @student.github %>"/><br>
    
  <label>Blog</label>
    <input type="TEXT" name="blog" value="<%= @student.blog %>"/><br>

  <label>Quote</label>  
    <input type="TEXT" name="quote" value="<%= @student.quote %>"/><br>

  <button type="submit">Change</button>
</form>



